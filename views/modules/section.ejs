<!-- 비디오 박스 시작-->
<a href="/channel/<%= live.channel.channelId %>" class="videoBox">
    <div class="videoBoxTop">
        <div class="videoBoxTopImg">
            <img class="liveImage" src="/imgs/free-icon-live-1246264.png" />
        </div>
        <div class="howManyWatch">
            <div class="getChannelId" hidden><%= live.channel.channelId %></div>
            현재 시청자수 : <%= live.channel.watchNum %>
        </div>
        <div class="videoThumbnail">
            <img src="/imgs/default/image.png" />
        </div>
    </div>
    <div class="videoBoxBottom">
        <div class="videoBoxBottomImgWrap">
            <img class="videoBoxBottomImg" src="<%= live.channel.channelImage %>" />
        </div>
        <div class="videoBoxBottomInfo">
            <div><%= live.title %></div>
            <div class="videoBoxBottomNickname"><%= live.channel.channelName %></div>
        </div>
    </div>
</a>

<script defer src="/script/chat.js"></script>

<!-- 비디오박스 끝 -->
